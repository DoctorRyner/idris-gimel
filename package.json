{
  "name": "gimel",
  "version": "0.0.1",
  "author": "Ryner Reinhardt <drryner@icloud.com>",
  "license": "MIT",
  "scripts": {
    "build": "sae release",
    "start-server": "parcel public/index.html",
    "watch": "fswatch src | (while read; do yarn build; done)",
    "release": "rm -rf dist && parcel build --no-source-maps public/index.html",
    "dev": "yarn build && concurrently 'yarn start-server' 'yarn watch'"
  },
  "dependencies": {
    "concurrently": "^6.2.0",
    "parcel": "^2.0.0-beta.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
